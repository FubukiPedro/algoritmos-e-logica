Algoritmo "teste_para"

Var

   n, i, valor, soma : inteiro

Inicio

      escreva("Quantos números serão digitados? ")
      leia(n)
      
      para i de 1 ate n faca
           escreva("Digite um número: ")
           leia(valor)
           soma <- soma + valor
      fimpara
      
      escreval("SOMA =", soma)

Fimalgoritmo